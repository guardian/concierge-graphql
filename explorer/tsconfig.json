{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "module": "ES2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
    "isolatedModules": true,
    "target": "ES2016",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react"
  },
  "include": ["./src/", "__tests__", "types.d.ts"],
  "exclude": ["node_modules"]
}